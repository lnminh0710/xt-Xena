{
    "name": "xena-ui",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "build_run": "ng build && dotnet run --project XenaUI.csproj",
        "watch_run": "ng serve --proxy-config proxy.config.json --live-reload=false",
        "server": "dotnet run --project XenaUI.csproj",
        "client": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --dev --watch",
        "client_aot": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --prod --aot",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "deploy": "npm run client_aot && dotnet restore XenaUI.csproj && dotnet build XenaUI.csproj --configuration Release && dotnet publish XenaUI.csproj --configuration Release"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "4.4.3",
        "@angular/cdk": "^2.0.0-beta.12",
        "@angular/common": "4.4.3",
